    "@langchain/openai": "^0.0.12",
    "@prisma/client": "^5.7.0",
    "bullmq": "^4.12.4",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "langchain": "^0.0.170",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@types/socket.io": "^3.0.2",
